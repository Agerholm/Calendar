<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="#FDFFFF"
    tools:context="com.yourdomain.calendar.ViewOversigt">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lørdag 16/5"
        android:id="@+id/textView2"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aktivitet"
        android:id="@+id/textView3"
        android:layout_marginTop="62dp"
        android:layout_below="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="25dp"
        android:lines="1"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Varighed"
        android:id="@+id/textView4"
        android:textSize="25dp"
        android:layout_alignTop="@+id/textView3"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Udført"
        android:id="@+id/textView5"
        android:textSize="25dp"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="23dp"
        android:textSize="25dp"
        android:textColor="#ff000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView18"
        android:textSize="25dp"
        android:text="10 min"
        android:layout_alignTop="@+id/textView14"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:background="@drawable/checkmark"
        android:layout_width="35dip"
        android:layout_height="35dip"
        android:id="@+id/imageButton"
        android:padding="-20dp"
        android:layout_alignTop="@+id/textView18"
        android:layout_toLeftOf="@+id/imageButton2"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:onClick="check"/>

    <ImageButton
        android:background="@drawable/cross"
        android:layout_width="35dip"
        android:layout_height="35dip"
        android:id="@+id/imageButton2"
        android:layout_alignTop="@+id/imageButton"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="uncheck"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Feje"
        android:id="@+id/textView19"
        android:layout_below="@+id/imageButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="35dp"
        android:textSize="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="20 min"
        android:id="@+id/textView20"
        android:layout_alignTop="@+id/textView19"
        android:layout_centerHorizontal="true"
        android:textSize="25dp" />

    <ImageButton
        android:background="@drawable/checkmark"
        android:layout_width="35dip"
        android:layout_height="35dip"
        android:id="@+id/imageButton3"
        android:padding="-20dp"
        android:onClick="check"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_toLeftOf="@+id/imageButton3"
        android:layout_toStartOf="@+id/imageButton3" />

    <ImageButton
        android:background="@drawable/cross"
        android:layout_width="35dip"
        android:layout_height="35dip"
        android:id="@+id/imageButton3"
        android:onClick="uncheck"
        android:layout_alignBottom="@+id/textView20"
        android:layout_toRightOf="@+id/imageButton"
        android:layout_toEndOf="@+id/imageButton" />


</RelativeLayout>
